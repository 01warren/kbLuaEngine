<root>
	<Properties>
		<name>
			<Type>			STRING			</Type>
			<Utype> 		41003			</Utype>
			<Flags>			ALL_CLIENTS		</Flags>
			<Default>		monster			</Default>
			<Persistent>		true			</Persistent>
			<DetailLevel>		MEDIUM			</DetailLevel>
		</name>

		<uid>
			<Type>			UINT32			</Type>
			<Utype> 		41004			</Utype>
			<Flags>			ALL_CLIENTS		</Flags>
			<Default>		0			</Default>
			<Persistent>		true			</Persistent>
		</uid>

		<utype>
			<Type>			UINT32			</Type>
			<Utype> 		41005			</Utype>
			<Flags>			ALL_CLIENTS		</Flags>
			<Default>		0			</Default>
			<Persistent>		true			</Persistent>
		</utype>

		<modelID>
			<Type>			UINT32			</Type>
			<Utype> 		41006			</Utype>
			<Flags>			ALL_CLIENTS		</Flags>
			<Default>		0			</Default>
		</modelID>
	</Properties>

	<CellMethods>
		<onTeleportSpaceCB>
			<Arg>		MAILBOX		</Arg>
			<Arg>		ENTITY_UTYPE	</Arg>
			<Arg>		POSITION3D	</Arg>
			<Arg>		DIRECTION3D	</Arg>
		</onTeleportSpaceCB>
	</CellMethods>
	
	<DetailLevels><!-- entity详情级别, 决定某些属性广播给客户端的优先级 -->
		<NEAR>
			<hyst>5</hyst>
			<radius>25</radius>
		</NEAR>
		<MEDIUM>
			<hyst>7</hyst>
			<radius>50</radius>
		</MEDIUM>
		<FAR>
			<hyst>10</hyst>
			<radius>75</radius>
		</FAR>
	</DetailLevels>
</root>
