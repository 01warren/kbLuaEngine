<root>
	<Volatile>
		<position/>
		<yaw/>
		<pitch> 20 </pitch>
	</Volatile>

	<Implements>
		<Interface>	GameObject		</Interface>
		<Interface>	Combat			</Interface>
		<Interface>	Spell			</Interface>
	</Implements>

	<Properties>
		<teleportingSpaceUType>
			<Type>			UINT32				</Type>
			<Flags>			CELL_PRIVATE			</Flags>
			<Persistent>		true				</Persistent>
		</teleportingSpaceUType>

		<spaceUType>
			<Utype> 41001 </Utype>
			<Type>			UINT32				</Type>
			<Flags>			CELL_PUBLIC_AND_OWN		</Flags>
			<Persistent>		true				</Persistent>
		</spaceUType>

		<level>
			<Utype> 41002 </Utype>
			<Type>			UINT16				</Type>
			<Flags>			CELL_PUBLIC_AND_OWN		</Flags>
			<Persistent>		true				</Persistent>
		</level>
	</Properties>

	<BaseMethods>
		<createCell>
			<Arg>		MAILBOX		</Arg>
		</createCell>
	</BaseMethods>

	<CellMethods>
		<queryCurrSpaceEntitys>
			<Exposed/>
			<Utype> 11002 </Utype>
			<Arg>		UINT8		</Arg>
		</queryCurrSpaceEntitys>
		<dialog>
			<Exposed/>
			<Utype> 11003 </Utype>
			<Arg>		ENTITY_ID	</Arg>
			<Arg>		UINT32		</Arg>
		</dialog>
		<queryCurrSpaceRes>
			<Exposed/>
			<Utype> 11004 </Utype>
		</queryCurrSpaceRes>
	</CellMethods>

	<ClientMethods>
		<onQueryEntityResult>
			<Utype> 10100			</Utype>
			<Arg>	ENTITY_INFOS 		</Arg>
		</onQueryEntityResult>

		<dialog_addOption>
			<Utype> 10101			</Utype>
			<Arg>	UINT8			</Arg>
			<Arg>	UINT32			</Arg>
			<Arg>	UNICODE			</Arg>
			<Arg>	INT32			</Arg>
		</dialog_addOption>

		<dialog_setText>
			<Utype> 10102			</Utype>
			<Arg>	UNICODE			</Arg>
			<Arg>	BOOL			</Arg>
			<Arg>	UINT32			</Arg>
			<Arg>	UNICODE			</Arg>
		</dialog_setText>

		<dialog_close>
			<Utype> 10104			</Utype>
		</dialog_close>

		<onQuerySpaceResResult>
			<Utype> 10103			</Utype>
			<Arg>	STRING	 		</Arg>
		</onQuerySpaceResResult>
	</ClientMethods>
</root>
